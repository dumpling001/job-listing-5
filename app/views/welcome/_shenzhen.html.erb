<!-- shenzhen -->
  <div class="row" style="margin: 20px 0px;">
    <div class="col-md-3 text-center">
      <a href="/jobs?workplace=深圳">
        <img class="img-circle" src="<%= image_url 'shenzhen.jpg' %>" alt="深圳">
      </a>
   </div>

  <div class="col-md-9">
    <table class="table table-hover">
      <thead>
         <tr>
           <td class="col-md-1">招聘职位</td>
           <td class="col-md-1">城市</td>
           <td class="col-md-1">收藏</td>
           <td class="col-md-1">点赞</td>
           <td class="col-md-1">薪资下限</td>
           <td class="col-md-1">薪资上限</td>
           <td class="col-md-1">刊登时间</td>
         </tr>
     </thead>
     <tbody>
     <% @sz.each do |job| %>
           <tr>
             <td><%= link_to(job.title, job_path(job)) %></td>
             <td><%= render_member_status(job) %></td>
             <td>
               <%= job.voters.count %>
               <%= render_voter_status(job) %>
             </td>
             <td><%= job.workplace.name %></td>
             <td><%= job.wage_lower_bound %></td>
             <td><%= job.wage_upper_bound %></td>
             <td><%= job.created_at.strftime("%Y-%m-%d")  %></td>
           </tr>
           <% end %>
    </tbody>
   </table>

  </div>
 </div>
<!-- shenzhen -->
